<!-- first section - Home -->
<div id="home" class="home">
    <div class="text-vcenter">
        <h1 style="font-family: cursive; font-style: normal ">WILL GIVE</h1>
        <h3>Donate A Lot</h3>
        <a href="#about" class="btn btn-default btn-lg">Charity Register</a>
    </div>
</div>
<!-- /first section -->

<!-- second section - About -->

<div id="about" class="pad-section">

    <div class="container">
        <div class="row">
            <div class="col-sm-6">
                <img src="/images/logo.jpg" alt="" />
            </div>

            <div class="col-sm-6 text-center">
                <h2>Make A Better World</h2>
                <p class="lead">Donation make the world better. Please donate as much as you have !!!</p>
            </div>
        </div>
    </div>
</div>
<!-- /second section -->


